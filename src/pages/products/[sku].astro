---
import Layout from '../../layout/Layout.astro';

export function getStaticPaths() {
  return [
    {params: {sku: 'sku-1'}},
    {params: {sku: 'sku-2'}},
    {params: {sku: 'sku-3'}},
  ];
}

const {sku} = Astro.params;
---

<Layout>
  <h1 class="text-2xl text-black dark:text-white">{sku}</h1>
</Layout>
